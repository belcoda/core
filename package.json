{
	"name": "belcoda",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev | npx pino-pretty",
		"build": "npx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ci": "vitest run --reporter=junit --reporter=default --outputFile=reports/tests/test-results.xml",
		"test:ui": "vitest --ui --coverage.enabled=true",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"zapatos": "node zapatos.js",
		"start:production": "node -r dotenv/config . | npx --yes pino-pretty",
		"ngrok": "node ngrok.config.cjs",
		"queue": "node queue.cjs",
		"knex:reset": "npx knex migrate:rollback --all --knexfile ./knexfile.cjs && npx knex migrate:latest --knexfile ./knexfile.cjs",
		"knex:migrate": "npx knex migrate:latest --knexfile ./knexfile.cjs",
		"knex:rollback": "npx knex migrate:rollback --knexfile ./knexfile.cjs",
		"knex:migrate:production": "npx knex migrate:latest --env production --knexfile ./knexfile.cjs",
		"postinstall": "npx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"inlang:lint": "npx @inlang/cli lint --project ./project.inlang",
		"inlang:translate": "npx @inlang/cli machine translate --project ./project.inlang --force"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "^2.0.0-beta.11",
		"@inlang/paraglide-vite": "^1.3.5",
		"@internationalized/date": "^3.6.0",
		"@melt-ui/svelte": "^0.86.2",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.9.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@types/codeflask": "^1.4.6",
		"@types/eslint": "^8.56.7",
		"@types/uuid": "^10.0.0",
		"@vitest/ui": "^1.6.0",
		"autoprefixer": "^10.4.19",
		"bits-ui": "^1.0.0-next.98",
		"dotenv": "^16.4.5",
		"embla-carousel-svelte": "^8.5.1",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0-next.13",
		"formsnap": "^2.0.0-next.1",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.468.0",
		"mode-watcher": "^0.5.0",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"svelte-hot-french-toast": "^1.0.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-superforms": "^2.21.1",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vaul-svelte": "^1.0.0-next.3",
		"vite": "^5.0.3",
		"vitest": "^1.2.0",
		"zod": "^3.24.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.608.0",
		"@aws-sdk/client-ses": "^3.609.0",
		"@aws-sdk/s3-request-presigner": "^3.608.0",
		"@sentry/sveltekit": "^8.36.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@tinymce/tinymce-svelte": "^3.0.0",
		"@types/knex": "^0.16.1",
		"@valibot/to-json-schema": "^1.0.0-beta.3",
		"@vitest/coverage-v8": "^1.6.0",
		"awesome-phonenumber": "^6.9.0",
		"clsx": "^2.1.1",
		"codeflask": "^1.4.1",
		"fast-csv": "^5.0.1",
		"google-auth-library": "^9.11.0",
		"graphile-worker": "^0.16.6",
		"handlebars": "^4.7.8",
		"isomorphic-dompurify": "^2.20.0",
		"javascript-time-ago": "^2.5.10",
		"knex": "^3.1.0",
		"letterparser": "^0.1.8",
		"node-pg-format": "^1.3.5",
		"openai": "^4.78.1",
		"pino": "^9.2.0",
		"pino-sentry-transport": "^1.5.0",
		"postgres": "^3.4.5",
		"redis": "^4.6.14",
		"runed": "^0.23.0",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"valibot": "^1.0.0-beta.3",
		"zapatos": "^6.4.0"
	}
}
